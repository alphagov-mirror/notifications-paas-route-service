---

applications:
  - name: <%= ENV.fetch("CF_APP_NAME") %>
    routes:
      - route: notify-route-service-<%= ENV.fetch("CF_SPACE") %>.cloudapps.digital
    buildpack: staticfile_buildpack
    instances: <%= ENV.fetch("CF_INSTANCES", "1") %>
    memory: 512M
    env:
      CF_SPACE: "<%= ENV.fetch("CF_SPACE") %>"
